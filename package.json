{
  "name": "dym-ts-sdk",
  "version": "1.0.0",
  "description": "Dymension sdk typescript",
  "contributors": [
    "mrtruongleo"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "files": [
    "lib",
    "*.md"
  ],
  "_moduleAliases": {
    "dym": "./src/"
  },
  "scripts": {
    "postinstall": "link-module-alias",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && link-module-alias",
    "clean": "npx tsc --build --clean",
    "test": "ts-node test.ts"
  },
  "dependencies": {
    "@injectivelabs/sdk-ts": "^1.14.5",
    "dym-ts-sdk": "github:mrtruongleo/dym-ts-sdk"
  },
  "devDependencies": {
    "link-module-alias": "^1.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
